{
    "name": "zachleigh/artisanize",
    "description": "Use Laravel Artisan command syntax in any Symfony Console project.",
    "keywords": [
        "console",
        "symfony",
        "artisan",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "symfony/console": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.1",
        "laravel/pint": "^1.24"
    },
    "autoload": {
        "psr-4": {
            "Artisanize\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
